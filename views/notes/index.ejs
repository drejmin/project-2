<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Note</th>
      <th>Class</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% notes.forEach(function(n) { %>
      <tr>
        <td><%= n.noteName %></td>
        <td><%= n.courseName %></td>
        <td><%= n.groupName %></td>
        <td><%= n.subject %></td>
        <td><a href="/notes/<%= n._id %>">Compose</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
